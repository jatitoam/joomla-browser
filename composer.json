{
    "name"          : "joomla-projects/joomla-browser",
    "description"   : "joomla-browser Codeception Module",
    "keywords"      : ["BDD", "acceptance testing", "tdd", "joomla"],
    "homepage"      : "https://docs.joomla.org/Testing_Joomla_Extensions_with_Codeception",
    "license"       : "GPL-2.0+",
    "authors"       : [
        {
          "name"    : "Puneet Kala",
          "email"   : "puneet.kala@community.joomla.org"
        },
        {
          "name"    : "Javier Gomez",
          "email"   : "javier.gomez@community.joomla.org"
        }
    ],
    "require": {
        "php"                                   : ">=5.5.9",
        "consolidation/robo"                    : "^1",
        "symfony/process"                       : "^3",
        "symfony/console"                       : "^3",
        "symfony/event-dispatcher"              : "^3",
        "symfony/filesystem"                    : "^3",
        "symfony/finder"                        : "^3",
        "symfony/dom-crawler"                   : "^3",
        "symfony/browser-kit"                   : "^3",
        "symfony/css-selector"                  : "^3",
        "symfony/yaml"                          : "^3",
        "symfony/debug"                         : "^3",
        "doctrine/instantiator"                 : "^1.0",
        "phpunit/php-code-coverage"             : "^5",
        "phpunit/php-timer"                     : "^1",
        "phpunit/phpunit-mock-objects"          : "^5"
    },
    "require-dev": {
        "joomla-projects/joomla-testing-robo"   : "dev-container-test",
        "codeception/codeception"               : "^2.4",
        "joomla/coding-standards"               : "^2"
    },
    "autoload": {
        "psr-4": {
            "Codeception\\Module\\"             : "src"
        }
    },
    "minimum-stability"                         : "dev",
    "prefer-stable"                             : true
}
