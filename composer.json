{
    "name"          : "joomla-projects/joomla-browser",
    "description"   : "joomla-browser Codeception Module",
    "keywords"      : ["BDD", "acceptance testing", "tdd", "joomla"],
    "homepage"      : "https://docs.joomla.org/Testing_Joomla_Extensions_with_Codeception",
    "license"       : "GPL-2.0+",
    "authors"       : [
        {
          "name"    : "Puneet Kala",
          "email"   : "puneet.kala@community.joomla.org"
        },
        {
          "name"    : "Javier Gomez",
          "email"   : "javier.gomez@community.joomla.org"
        }
    ],
    "require": {
        "php"                                   : ">=5.5.9",
        "consolidation/robo"                    : "~1",
        "symfony/process"                       : "<4",
        "symfony/console"                       : "<4",
        "symfony/event-dispatcher"              : "<4",
        "symfony/filesystem"                    : "<4",
        "symfony/finder"                        : "<4",
        "symfony/dom-crawler"                   : "<4",
        "symfony/browser-kit"                   : "<4",
        "symfony/css-selector"                  : "<4",
        "symfony/yaml"                          : "<4"
    },
    "require-dev": {
        "joomla-projects/joomla-testing-robo"   : "dev-container-test",
        "codeception/codeception"               : "^2.4",
        "joomla/coding-standards"               : "^2"
    },
    "autoload": {
        "psr-4": {
            "Codeception\\Module\\"             : "src"
        }
    },
    "minimum-stability"                         : "dev",
    "prefer-stable"                             : true
}
